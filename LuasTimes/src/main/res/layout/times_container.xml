<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/timesContainer"
    style="@style/Fill.Height"
    android:orientation="vertical" >

    <include layout="@layout/include_times_header" />

    <View style="@style/BlueLine" />

    <TableLayout
        android:id="@+id/tblInbound"
        style="@style/Fill"
        android:layout_weight=".5" />

    <include layout="@layout/include_times_header" />

    <View style="@style/BlueLine" />

    <TableLayout
        android:id="@+id/tblOutbound"
        style="@style/Fill"
        android:layout_weight=".5" />

</LinearLayout>
